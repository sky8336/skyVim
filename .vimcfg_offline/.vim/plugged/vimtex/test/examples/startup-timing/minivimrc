set nocompatible
let &rtp  = '~/.vim/bundle/vimtex,' . &rtp
let &rtp .= ',~/.vim/bundle/vimtex/after'
filetype plugin indent on
syntax enable

nnoremap q :qall!<cr>

let g:vimtex_toc_todo_labels = {
      \ 'TODO': 'TODO: ',
      \ 'FIXME': 'FIXME: ',
      \ 'IMPORTANT': 'IMPORTANT: ',
      \ 'IMPORTANTE': 'IMPORTANT: ',
      \}

silent edit thesis.tex

" VimtexTocOpen
" echo ''
quitall
